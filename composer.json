{
    "name": "mramsden/tefel",
    "description": "Parses the station facilities feed from TFL to create a list of stations, with their location and the lines serving them.",
    "authors": [
        {
            "name": "Marcus Ramsden",
            "email": "marcus.ramsden@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "4.2.*",
        "guzzle/guzzle": "~3.9"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Tefel\\": "src/Tefel"
        }
    },
    "minimum-stability": "stable"
}
