{
    "name": "mramsden/tefel",
    "description": "Parses the station facilities feed from TFL to create a list of stations, with their location and the lines serving them.",
    "version": "0.1.1",
    "keywords": ["data", "feed", "london", "laravel", "underground"],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcus Ramsden",
            "email": "marcus.ramsden@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/mramsden/tefel/issues",
        "source": "https://github.com/mramsden/tefel"
    },
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "4.2.*",
        "guzzle/guzzle": "~3.9"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "benconstable/phpspec-laravel": "~1.0",
        "henrikbjorn/phpspec-code-coverage": "~0.2",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Tefel\\": "src/Tefel",
            "spec\\": "spec"
        }
    },
    "minimum-stability": "stable"
}
